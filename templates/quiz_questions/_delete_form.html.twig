<form method="post" action="{{ path('app_quiz_questions_delete', {'QuestID': q.QuestID}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ q.QuestID) }}">
   <button class="btn btn-danger shadow btn-xs sharp "><i class="fa fa-trash"></i>  </button>
</form>
